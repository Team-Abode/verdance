{
    "multipart": [
        {
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_base"
            }
        },
        {
            "when": {
                "OR": [
                    {
                        "north": "true"
                    },
                    {
                        "south": "true"
                    }
                ]
            },
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_stalk"
            }
        },
        {
            "when": {
                "OR": [
                    {
                        "east": "true"
                    },
                    {
                        "west": "true"
                    }
                ]
            },
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_stalk",
                "y": 90,
                "uvlock": true
            }
        },
        {
            "when": {
                "north": "true"
            },
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_attachment",
                "uvlock": true
            }
        },
        {
            "when": {
                "east": "true"
            },
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_attachment",
                "y": 90,
                "uvlock": true
            }
        },
        {
            "when": {
                "south": "true"
            },
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_attachment",
                "y": 180,
                "uvlock": true
            }
        },
        {
            "when": {
                "west": "true"
            },
            "apply": {
                "model": "verdance:block/attached_cantaloupe_stem_attachment",
                "y": 270,
                "uvlock": true
            }
        }
    ]
}